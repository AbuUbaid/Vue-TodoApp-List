);\r\n   },\r\n\r\n   recieveData: function($event){\r\n     this.myData = $event;\r\n     console.log(\"data received..\");\r\n     console.log(this.myData);\r\n   }\r\n   \r\n  \r\n    \r\n\r\n   \r\n  }\r\n  \r\n\r\n})\r\n</script>\r\n\r\n"],"sourceRoot":"src/components"}]}